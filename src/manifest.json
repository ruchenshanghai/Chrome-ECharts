{
  "name": "Chrome Data Visualization",
  "version": "1.0",
  "description": "Build an Extension to visualize the data on the website!",
  "permissions": ["declarativeContent", "storage", "https://unpkg.com/vue/dist/*", "https://unpkg.com/element-ui/*"],
//  "background": {
//    "scripts": ["js/background.js"],
//    "persistent": false
//  },
  "browser_action": {
    // page or browser + action
    "default_popup": "html/default_popup.html",
    "default_icon": {
      "16": "assets/images/get_started16.png",
      "32": "assets/images/get_started32.png",
      "48": "assets/images/get_started48.png",
      "128": "assets/images/get_started128.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E"
      },
      "description": "Opens default_popup.html"
    }
  },
    "icons": {
    "16": "assets/images/get_started16.png",
    "32": "assets/images/get_started32.png",
    "48": "assets/images/get_started48.png",
    "128": "assets/images/get_started128.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-qOmN3IJ0L9HH+B68LTdoTsDwG/U8m6WIngXVnzd02/I=' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2
}